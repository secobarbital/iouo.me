{render, body, coffeescript} = require './'

console.log render ->
  body ->
    coffeescript ->
      class Foo
        name: -> 'Foo'
        hello: (args...) ->
          "#{@name()} says #{args.length}"

      class Bar extends Foo
        name: -> 'Bar'

      console.log new Bar().hello(1,2,3)




