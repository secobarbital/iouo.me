{
  "name": "iouo.me",
  "subdomain": "iouo",
  "domains": [
    "www.iouo.me"
  ],
  "version": "2.0.0",
  "private": true,
  "main": "server.js",
  "browser": "src/index.jsx",
  "scripts": {
    "start": "node ./bin/www",
    "build": "NODE_ENV=production node_modules/.bin/browserify src/index.jsx --extension=.jsx -p bundle-collapser/plugin -g uglifyify | node_modules/uglifyify/node_modules/.bin/uglifyjs -m -c --screw-ie8 -o public/bundle.js",
    "watch": "node_modules/.bin/watchify src/index.jsx -o public/bundle.js --extension=.jsx -d -v",
    "postinstall": "npm run build"
  },
  "engines": {
    "iojs": "1.6.x",
    "npm": "2.7.x"
  },
  "dependencies": {
    "browserify": "^9.0.3",
    "bundle-collapser": "^1.1.4",
    "compression": "^1.4.3",
    "connect-cachify": "0.0.17",
    "debug": "^2.1.3",
    "envify": "^3.4.0",
    "events": "^1.0.2",
    "express": "^4.12.3",
    "flux": "^2.0.1",
    "immutable": "^3.6.4",
    "intl": "^0.1.4",
    "jade": "^1.9.2",
    "keymirror": "^0.1.1",
    "morgan": "^1.5.2",
    "object-assign": "^2.0.0",
    "react": "^0.12.2",
    "react-intl": "^1.2.0",
    "react-router": "^0.12.4",
    "reactify": "^1.1.0",
    "rollbar": "^0.4.5",
    "serve-favicon": "^2.2.0",
    "superagent": "^0.21.0",
    "twitter": "^1.2.5",
    "uglifyify": "^3.0.1"
  },
  "devDependencies": {
    "watchify": "^2.6.2"
  },
  "browserify": {
    "transform": [
      "envify",
      [
        "reactify",
        {
          "es6": true
        }
      ]
    ]
  }
}
