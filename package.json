{
  "name": "iouo.me",
  "subdomain": "iouo",
  "domains": [
    "www.iouo.me"
  ],
  "version": "3.0.0",
  "private": true,
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "standard": "standard src",
    "prebrowserify": "mkdir -p dist && npm run standard",
    "browserify": "browserify src/index.js -t babelify -g uglifyify -o dist/iouome.js",
    "prewatch": "mkdir -p dist",
    "watch": "watchify src/index.js -t babelify -o dist/iouome.js -d -v --poll",
    "start": "npm install && npm run watch"
  },
  "dependencies": {
    "@cycle/core": "^5.0.1",
    "@cycle/dom": "^7.1.1",
    "@cycle/fetch": "^4.0.0",
    "cycle-pushstate-driver": "^3.0.0",
    "cycle-vtree-switcher": "^2.0.0",
    "es6-promise": "^3.0.2",
    "rx": "^4.0.6",
    "whatwg-fetch": "^0.10.0"
  },
  "engines": {
    "node": "4.1.x",
    "npm": "2.14.x"
  },
  "devDependencies": {
    "babel": "^5.8.23",
    "babelify": "^6.4.0",
    "browserify": "^11.2.0",
    "standard": "^5.3.1",
    "tape": "^4.2.2",
    "uglifyify": "^3.0.1",
    "watchify": "^3.4.0"
  }
}
