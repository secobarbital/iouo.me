{
  "name": "iouo.me",
  "subdomain": "iouo",
  "domains": [
    "www.iouo.me"
  ],
  "version": "3.0.0",
  "private": true,
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "standard": "node_modules/.bin/standard",
    "prebrowserify": "mkdir -p dist && npm run standard",
    "browserify": "browserify src/index.js -t babelify -o dist/iouome.js",
    "prewatch": "mkdir -p dist",
    "watch": "watchify src/index.js -t babelify -o dist/iouome.js -d -v --poll",
    "start": "npm install && npm run watch"
  },
  "dependencies": {
    "@cycle/core": "^3.1.0",
    "@cycle/dom": "^5.3.1",
    "@cycle/fetch": "^1.0.0",
    "cycle-pushstate-driver": "^1.0.2",
    "cycle-route": "^2.0.1",
    "es6-promise": "^3.0.2",
    "whatwg-fetch": "^0.9.0"
  },
  "engines": {
    "node": "4.1.x",
    "npm": "2.14.x"
  },
  "devDependencies": {
    "babel": "^5.8.23",
    "babelify": "^6.3.0",
    "browserify": "^11.2.0",
    "standard": "^5.3.1",
    "tape": "^4.2.1",
    "watchify": "^3.4.0"
  }
}
