{
  "name": "iouo.me",
  "subdomain": "iouo",
  "domains": [
    "www.iouo.me"
  ],
  "version": "2.0.0",
  "private": true,
  "main": "server.js",
  "browser": "src/index.jsx",
  "scripts": {
    "start": "node ./bin/www",
    "build": "NODE_ENV=production node_modules/.bin/browserify src/index.jsx -o public/bundle.js --extension=.jsx -t envify -t [ reactify --es6 ] -p bundle-collapser/plugin -g uglifyify",
    "watch": "node_modules/.bin/watchify src/index.jsx -o public/bundle.js --extension=.jsx -t 6to5ify -d -v",
    "postinstall": "npm run build"
  },
  "engines": {
    "iojs": "1.1.x",
    "npm": "2.4.x"
  },
  "dependencies": {
    "bootstrap": "^3.3.2",
    "connect-cachify": "0.0.17",
    "debug": "^2.1.1",
    "events": "^1.0.2",
    "express": "^4.11.2",
    "flux": "^2.0.1",
    "immutable": "^3.6.2",
    "jade": "^1.9.2",
    "keymirror": "^0.1.1",
    "morgan": "^1.5.1",
    "nano": "^6.0.2",
    "object-assign": "^2.0.0",
    "react": "^0.12.2",
    "react-intl": "^1.1.0",
    "react-router": "^0.12.0",
    "serve-favicon": "^2.2.0",
    "superagent": "^0.21.0"
  },
  "devDependencies": {
    "browserify": "^8.1.3",
    "bundle-collapser": "^1.1.1",
    "envify": "^3.2.0",
    "reactify": "^1.0.0",
    "uglifyify": "^3.0.1",
    "watchify": "^2.3.0"
  }
}
